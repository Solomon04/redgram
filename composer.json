{
    "name": "redgram/redgram",
    "description": "Redgram",
    "keywords": ["redgram", "reddit", "laravel zero", "instagram", "cli"],
    "homepage": "https://github.com/Solomon04/redgram",
    "type": "project",
    "license": "MIT",
    "support": {
        "issues": "https://github.com/Solomon04/redgram/issues",
        "source": "https://github.com/Solomon04/redgram"
    },
    "authors": [
        {
            "name": "Solomon Antoine",
            "email": "antoinesolomon5@gmail.com"
        }
    ],
    "require": {
        "php": "^7.4",
        "ext-json": "*",
        "illuminate/encryption": "^5.8",
        "laravel-zero/framework": "5.8.*",
        "mgp25/instagram-php": "^7.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.0",
        "nunomaduro/larastan": "^0.4.3",
        "phpunit/phpunit": "^7.5"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "scripts": {
        "post-create-project-cmd": [
            "@php application app:rename"
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "bin": ["redgram"]
}
